repeat wait() until game:IsLoaded()
local PlaceId = game.PlaceId
  
if PlaceId == 5780309044 then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/XInfiniteHub/teste/main/Infinite%20Hub%20(SA)"))()
elseif PlaceId == 11423467063 then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/XInfiniteHub/teste/main/Auto%20Boss"))()
end
