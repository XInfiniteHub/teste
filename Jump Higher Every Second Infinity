--// Ui Libray \\--
local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/XInfiniteHub/teste/main/Infintie%20Hub%20Libray%20S"))()
local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()


--// Notification \\--
Notification:Notify(
    {Title = "Script By Infinity hub", Description = "Devs: @Darkzin, @Cool"},
    {OutlineColor = Color3.fromRGB(80, 80, 80),Time = 5, Type = "default"}
)


--// Main Code \\--
--[[SolarisLib:New({
  Name - Title of the UI <string>
  FolderToSave - Name of the folder where the UI files will be stored <string>
})]]
local win = SolarisLib:New({
  Name = "SolarisLib",
  FolderToSave = "SolarisLibStuff"
})

--// Tabs
local autfw = win:Tab("Auto Farm Wins")

--// Auto Farm Wins Main
local sec = autfw:Section("Auto Farm Wins Main")
local toggle = sec:Toggle("Start / Stop Farm", false,"Toggle", function(autfw)
    if autfw then
        getgenv().AutoFarmToggle = true
        while getgenv().AutoFarmToggle do
            wait(1)
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(1211, 123468, 8271))
        end
    else
        getgenv().AutoFarmToggle = false
    end
end)
