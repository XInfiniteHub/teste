	Notification:Notify(
    	{Title = "Game not detected", Description = "..."},
    	{OutlineColor = Color3.fromRGB(80, 80, 80),Time = 5, Type = "default"}
	)

	wait(0.5)

	game.Players.LocalPlayer:Kick("Game not detected, join the discord server to see the games where the project hub works")
	
end
