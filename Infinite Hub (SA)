		if fps then
			getgenv().fpsw = true
			while getgenv().fpsw do
				wait(0.1)
				local FPS = 100
				local clock = tick()
	
				while true do
				    while clock + 1 / FPS > tick() do end
				    wait()
				    clock = tick()
				end
			end
		else
			getgenv().fpsw = false
		end
	end)
	
