	--// Local Player \\--
	local sec = lp:Section("--<    Local Player Main    >--")
	local toggle = sec:Toggle("Ctrl + Click Tp", false,"Toggle", function(CtrlTp)
	  	if CtrlTp then
			  getgenv().CtrlTpw = true
			  while getgenv().CtrlTpw do
				wait(0.1)
				local Plr = game:GetService("Players").LocalPlayer
				local Mouse = Plr:GetMouse()
	
				Mouse.Button1Down:connect(function()
				if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
				if not Mouse.Target then return end
				Plr.Character:MoveTo(Mouse.Hit.p)
				end)
			  end
			else
				getgenv().CtrlTpw = false
		  end
	end)
	local toggle = sec:Toggle("Ctrl + Click Destroy", false,"Toggle", function(CtrlDes)
	  	if CtrlDes then
			  getgenv().CtrlDesw = true
			  while getgenv().CtrlDesw do
				wait(0.1)
				local Plr = game:GetService("Players").LocalPlayer
				local Mouse = Plr:GetMouse()
	
				Mouse.Button1Down:connect(function()
				if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
				if not Mouse.Target then return end
				Mouse.Target:Destroy()
				end)
			  end
			else
				getgenv().CtrlDesw = false
		  end
	end)
	local slider = sec:Slider("WalkSpeed", 16,500,16,2.5,"Speed", function(speed)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
	end)
	local slider = sec:Slider("JumpPower", 50,500,50,2.5,"Jump", function(jump)
