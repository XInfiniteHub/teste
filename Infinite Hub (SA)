	local sec = it:Section("--<    Item Farm Main    >--")
	local toggle = sec:Toggle("Start / Stop Item Farm", false,"Toggle", function(start)
		if start then
			getgenv().startw = true
			while getgenv().startw do
				wait(0.1)
				for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
					if v:IsA("Tool") and v:FindFirstChild("Handle") then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
					end
				end
			end
		else
			getgenv().startw = false
		end
	end)
	local toggle = sec:Toggle("Grab Tools", false,"Toggle", function(grab)
		if grab then
			getgenv().grabw = true
			while getgenv().grabw do
				wait(0.1)
				print("soon...")
			end
		else
			getgenv().grabw = false
		end
	end)
	
	
